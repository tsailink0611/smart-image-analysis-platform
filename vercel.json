{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "routes": [
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "/index.html" }
    ],
    "functions": {
      "api/**/*.{js,mjs}": { "runtime": "nodejs18.x", "maxDuration": 60 }
    },
    "env": { "NODE_VERSION": "18" }
  }